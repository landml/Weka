[run]
branch = True

omit =
    /kb/module/lib/Weka/authclient.py
    /kb/module/lib/Weka/baseclient.py
    /kb/module/lib/Weka/WekaClient.py
    /kb/module/lib/Weka/WekaServer.py
source = /kb/module/lib/Weka


[report]
# Regexes for lines to exclude from consideration
exclude_lines =
    # Have to re-enable the standard pragma
    pragma: no cover
    # Don't complain if tests don't hit defensive assertion code:
    raise NotImplementedError

show_missing = 1
